{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/BooksArchivingSystem.csproj"],
			"group": "build",
			"isBackground": false,
			"problemMatcher": ["$msCompile"]
		},
		{
			"label": "run",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"${workspaceFolder}/BooksArchivingSystem.csproj"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": []
		},
		{
			"label": "watch",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/BooksArchivingSystem.csproj"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": []
		},
		{
			"label": "restore",
			"type": "shell",
			"command": "dotnet",
			"args": ["restore"],
			"group": "build",
			"isBackground": false,
			"problemMatcher": []
		}
	]
}
